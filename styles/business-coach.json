          "size": "3.4375rem"
        },
        {
          "name": "65px",
          "slug": "65px",
          "size": "4.0625rem"
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var:preset|font-family|schibsted-grotesk",
      "fontSize": "18px",
      "lineHeight": "1.6"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var:preset|font-family|schibsted-grotesk",
          "fontSize": "65px",
          "fontWeight": "700",
          "lineHeight": "1.2"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var:preset|font-family|schibsted-grotesk",
          "fontSize": "40px",
          "fontWeight": "500",
          "lineHeight": "50px"
        }
      }
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": "10px",
          "width": "1px",
          "color": "var:preset|color|accent-2",
          "style": "solid"
        },
        "typography": {
          "fontFamily": "var:preset|font-family|schibsted-grotesk",
          "fontWeight": "500",
          "fontSize": "var(--wp--preset--font-size--18px)",
          "textAlign": "center"
        },
        "spacing": {
          "padding": {
            "top": "0.625rem",
            "bottom": "0.625rem",
            "left": "1.25rem",
            "right": "1.25rem"
          }
        },
        "color": {
          "background": "var:preset|color|accent-2",
          "text": "var:preset|color|accent-1"
        },
        "variations": {
          "outline": {
            "border": {
              "color": "var:preset|color|accent-2",
              "radius": "10px",
              "width": "1px",
              "style": "solid"
            },
            "color": {
              "background": "transparent",
              "text": "var:preset|color|base"
            },
            "shadow": "none",
            "css": ".wp-block-button__link:not(.has-background):hover {background-color: var(--wp--preset--color--accent-2);}",
            "spacing": {
              "padding": {
                "top": "0.625rem",
                "bottom": "0.625rem",
                "left": "1.25rem",
                "right": "1.25rem"
              }
            },
            "typography": {
              "fontFamily": "var:preset|font-family|schibsted-grotesk",
              "fontWeight": "500",
              "fontSize": "var(--wp--preset--font-size--18px)",
              "textAlign": "center"
            }
          }
        }
      }
    }
  }
}
