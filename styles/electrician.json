			"fontSize": "var:preset|font-size|18px",
			"lineHeight": "1.6"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var:preset|font-family|schibsted-grotesk",
					"fontSize": "var:preset|font-size|65px",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var:preset|font-family|schibsted-grotesk",
					"fontSize": "var:preset|font-size|40px",
					"fontWeight": "500",
					"lineHeight": "50px"
				}
			}
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": "10px",
					"width": "1px",
					"color": "var:preset|color|peach",
					"style": "solid"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|schibsted-grotesk",
					"fontWeight": "500",
					"fontSize": "18px",
					"textAlign": "center"
				},
				"spacing": {
					"padding": {
						"top": "10px",
						"bottom": "10px",
						"left": "20px",
						"right": "20px"
					}
				},
				"color": {
					"background": "var:preset|color|peach",
					"text": "var:preset|color|dark-blue"
				},
				"variations": {
					"outline": {
						"border": {
							"color": "var:preset|color|peach",
							"radius": "10px",
							"width": "1px",
							"style": "solid"
						},
						"color": {
							"background": "transparent",
							"text": "var:preset|color|white"
						},
						"shadow": "none",
						"spacing": {
							"padding": {
								"top": "10px",
								"bottom": "10px",
								"left": "20px",
								"right": "20px"
							}
						},
						"typography": {
							"fontFamily": "var:preset|font-family|schibsted-grotesk",
							"fontWeight": "500",
							"fontSize": "var:preset|font-size|18px",
							"textAlign": "center"
						}
					}
				}
			}
		}
